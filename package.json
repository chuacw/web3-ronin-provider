{
  "name": "web3-ronin-provider",
  "version": "1.0.9",
  "types": "./dist/index.d.ts",
  "main": "index.js",
  "scripts": {
    "gen-markdown": "typedoc --cleanOutputDir true --plugin typedoc-plugin-markdown --out docs/md src",
    "gen-html": "typedoc --cleanOutputDir true --out docs/html src",
    "prepublishOnly": "tsc -b && npm run gen-markdown && npm run gen-html",
    "test": "jest"
  },
  "exports": {
    ".": "./dist/index.js",
    "./web3-ronin-consts": "./dist/web3-ronin-consts.js",
    "./web3-RoninJsonRpcProvider": "./dist/web3-RoninJsonRpcProvider.js",
    "./web3-RoninSkynetProvider": "./dist/web3-RoninSkynetProvider.js"
  },
  "keywords": [
    "Ronin",
    "Web3",
    "Provider",
    "SkyNet",
    "API"
  ],
  "author": "chuacw",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chuacw/web3-ronin-provider/issues"
  },
  "homepage": "https://github.com/chuacw/web3-ronin-provider#readme",
  "description": "A Ronin JSON RPC and SkyNet Web3 provider",
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "axios": "^1.7.7",
    "delphirtl": "^1.1.20",
    "ethers": "5.7"
  },
  "files": [
    "dist/**/*",
    "docs/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typedoc": "^0.26.10",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.6.3"
  }
